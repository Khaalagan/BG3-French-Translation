	<content contentuid="h834b4f39g71bcg4f66g824cg72dbd1cf5adc" version="1">Cape du Magicien des ténèbres</content>
	<content contentuid="hbf0c608cg203dg4da6g98adg40fe62cea36c" version="1">Marque du Magicien des ténèbres</content>
