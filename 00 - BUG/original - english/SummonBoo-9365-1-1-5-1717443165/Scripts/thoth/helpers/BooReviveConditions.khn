local __util = require 'larian.util'

function IsTargetBunny(target)
    entity = target or context.Target
    result = Tagged('IS_RABBIT',entity)
    return ConditionResult(result.Result,{ConditionError("TargetNotABunny")})
end

function HasGoldForBlackJack(entity)
    entity = entity or context.Source
    result = HasStatus('BOO_BLACKJACK_GOLDCOST',entity)
    return ConditionResult(result.Result,{ConditionError("NotHasGoldForBlackJack")})
end